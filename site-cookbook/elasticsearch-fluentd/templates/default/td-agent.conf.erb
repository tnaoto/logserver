<source>
	type tail
	format nginx
	path /var/log/nginx/localhost.access.log
	tag foo01.nginx.access
</source>

<match *.nginx.*>
	index_name adminpack
	type_name nginx
	type elasticsearch
	include_tag_key true
	tag_key @log_name
	host localhost
	port 9200
	logstash_format true
	flush_interval 10s
</match>    
